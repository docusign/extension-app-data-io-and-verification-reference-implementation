namespace org.example@1.0.0

@VerifiableType
@Term('Email Address')
concept VerifyEmailInput {
    @IsRequiredForVerifyingType
    @Term('Email Address')
    o String email length=[,256]
}

@VerifiableType
@Term("Full Name")
concept VerifyFullNameInput {
    @IsRequiredForVerifyingType
    @Term("First Name")
    o String firstName

    @IsRequiredForVerifyingType
    @Term("Last Name")
    o String lastName
}
