{
  "name": "Combined app",
  "description": {
    "short": "App for data verification and io and Connected Fields",
    "long": "This app is designed to allow for users to verify different kinds of data fields with the database values, use data io extensions and use connected fields extensions"
  },
  "icon": {
    "data": "",
    "mediaType": "image/png"
  },
  "screenshots": [],
  "publisher": {
    "name": "John Doe",
    "email": "john.doe@gmail.com",
    "phone": "800-867-5309",
    "website": "www.johndoe.com"
  },
  "termsOfServiceUrl": "www.johndoe.com/tos",
  "privacyUrl": "www.johndoe.com/privacy",
  "supportUrl": "www.johndoe.com/support",
  "connections": [
    {
      "name": "authentication",
      "description": "secure connection to the data verification and io proxy",
      "type": "oauth2",
      "params": {
        "provider": "CUSTOM",
        "scopes": [],
        "clientId": "<CLIENT_ID>",
        "clientSecret": "<CLIENT_SECRET>",
        "customConfig": {
          "tokenUrl": "<PROXY_BASE_URL>/api/oauth/token",
          "authorizationUrl": "<PROXY_BASE_URL>/api/oauth/authorize",
          "authorizationParams": {
            "access_type": "offline",
            "prompt": "consent"
          },
          "authorizationMethod": "header",
          "scopeSeparator": " ",
          "requiredScopes": []
        }
      }
    }
  ],
  "extensions": [
    {
      "name": "My Connected Fields Extension",
      "description": "Used to verify and autofill agreements with custom data models",
      "template": "ConnectedFields.Version1.ConnectedFields",
      "actionReferences": [
        "My Verify Action",
        "My GetTypeNames Action",
        "My GetTypeDefinitions Action"
      ]
    },
    {
      "name": "My Data IO Extension",
      "description": "using in data io flow",
      "template": "DataIO.Version6.DataInputOutput",
      "actionReferences": [
        "CreateRecord Action",
        "PatchRecord Action",
        "SearchRecords Action",
        "GetTypeNames Action",
        "GetTypeDefinitions Action"
      ]
    },
    {
      "name": "My Bank Account Verification Extension",
      "description": "Verify bank account",
      "template": "Verify.Version3.BankAccount",
      "actionReferences": [
        "My Bank Account Verification Action"
      ]
    },
    {
      "name": "My Business FEIN Verification Extension",
      "description": "Verify business FEIN",
      "template": "Verify.Version1.BusinessEntity",
      "actionReferences": [
        "My Business FEIN Verification Action"
      ]
    },
    {
      "name": "My Email Verification Extension",
      "description": "Verify email",
      "template": "Verify.Version1.Email",
      "actionReferences": [
        "My Email Verification Action"
      ]
    },
    {
      "name": "My Phone Verification Extension",
      "description": "Verify phone number",
      "template": "Verify.Version1.PhoneNumber",
      "actionReferences": [
        "My Phone Verification Action"
      ]
    },
    {
      "name": "My Postal Address Verification Extension",
      "description": "Verify postal address",
      "template": "Verify.Version1.PostalAddress",
      "actionReferences": [
        "postal-address-verify",
        "postal-address-suggestion"
      ]
    },
    {
      "name": "My SSN Verification Extension",
      "description": "Verify SSN",
      "template": "Verify.Version2.SocialSecurityNumber",
      "actionReferences": [
        "My SSN Verification Action"
      ]
    },
    {
      "name": "My Bank Account Owner Verification Extension",
      "description": "Verify bank account owner",
      "template": "Verify.Version1.BankAccountOwner",
      "actionReferences": [
        "My Bank Account Owner Verification Action"
      ]
    }
  ],
  "actions": [
    {
      "name": "My Verify Action",
      "description": "This is a description of my verify action",
      "template": "ConnectedFields.Version1.Verify",
      "connectionsReference": "authentication",
      "params": {
        "uri": "<PROXY_BASE_URL>/api/connectedfields/verify"
      }
    },
    {
      "name": "My GetTypeNames Action",
      "description": "This is a description of my GetTypeNames action",
